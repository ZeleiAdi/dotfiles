set nocompatible

set hidden

filetype plugin indent on

syntax enable
set background=dark
colorscheme solarized

set shortmess+=I

set number
set cursorline
set ruler

set linebreak

set expandtab
set shiftwidth=2
set softtabstop=2

set ignorecase smartcase
set incsearch
set hlsearch

set undofile

set showmatch

map Y y$
